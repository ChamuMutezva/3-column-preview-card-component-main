rder: 1px solid transparent;\r\n        font-weight: 600;\r\n        margin-top: 24px;\r\n        margin-bottom: 49px;\r\n    }\r\n    @include abstracts.mQ(1080px){\r\n        margin: 0;\r\n        max-width: 307px;\r\n        border-top-left-radius: 8px;\r\n        border-top-right-radius: 0px;\r\n        border-bottom-left-radius: 8px;\r\n        &:nth-child(n+2) {\r\n            top: 0px;\r\n            left: -10px;\r\n        }\r\n    \r\n        &:last-child {\r\n            top: 0px;\r\n            left: -20px;\r\n        }\r\n       \r\n    \r\n        .card--btn {\r\n            margin-top: 78px;\r\n        }\r\n    }\r\n}\r\n\r\n.sedan {\r\n    background-color: abstracts.$bg-orange;\r\n    .card--btn {\r\n        color: abstracts.$tx-orange;\r\n    }\r\n}\r\n\r\n.suv {\r\n    background-color: abstracts.$bg-cyan;\r\n    .card--btn {\r\n        color: abstracts.$tx-cyan;\r\n    }\r\n}\r\n\r\n.luxury {\r\n    background-color: abstracts.$bg-green;\r\n     .card--btn {\r\n        color: abstracts.$tx-green;\r\n    }\r\n\r\n}","scss\\abstracts\\_mixins.scss":"@mixin mQ($arg) {\r\n    @media screen and (min-width: $arg) {\r\n        @content;\r\n    }\r\n}","scss\\abstracts\\_variables.scss":"$main-font: 'Lexend Deca', sans-serif;\r\n$secondary-font: 'Big Shoulders Display', cursive;\r\n$bg-main-grey: #f2f2f2;\r\n//background color\r\n$bg-orange: hsl(31, 77%, 52%);\r\n$bg-cyan: hsl(184, 100%, 22%);\r\n$bg-green: hsl(179, 100%, 13%);\r\n//text color\r\n$tx-orange: hsl(31, 77%, 52%);\r\n$tx-cyan: hsl(184, 100%, 22%);\r\n$tx-green: hsl(179, 100%, 13%);\r\n","scss\\layouts\\_main.scss":"@use '../abstracts';\r\n\r\nmain {\r\n    padding-top: 88px;\r\n    padding-bottom: 24px;\r\n    position: relative;\r\n    \r\n\r\n    @include abstracts.mQ(1080px) {\r\n        padding-top: 171px;\r\n        display: flex;\r\n        flex-direction: row;\r\n        flex-wrap: nowrap;\r\n        justify-content: center;\r\n        align-items: center;\r\n    }\r\n}\r\n\r\n.wrapper